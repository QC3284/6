{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/proxy?url=https://cdn.xcqcoo.top/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Proxy-By", "value": "Vercel" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/proxy", "dest": "/api/proxy" },
    { "src": "/(.*)", "dest": "/api/proxy?url=https://cdn.xcqcoo.top/$1" }
  ],
  "functions": {
    "api/proxy.js": {
      "memory": 3008,
      "maxDuration": 30,
      "includeFiles": "node_modules/zlib/**"
    }
  }
}
