{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/$1",
      "status": 200,
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/proxy?url=https://cdn.xcqcoo.top/$1",
      "headers": {
        "X-Proxy-By": "Vercel"
      }
    }
  ]
}
